# Agentic-Flow Docker Environment Configuration
# Copy this file to .env and fill in your API keys

# ============================================
# API Keys (at least one required)
# ============================================

# Anthropic (Recommended for best quality)
ANTHROPIC_API_KEY=sk-ant-your-key-here

# OpenRouter (99% cost savings)
OPENROUTER_API_KEY=sk-or-your-key-here

# Google Gemini (Free tier available)
GOOGLE_GEMINI_API_KEY=your-gemini-key-here

# ============================================
# Provider Configuration
# ============================================

# Default provider: anthropic, openrouter, gemini, or onnx (local)
DEFAULT_PROVIDER=anthropic

# Default model (provider-specific)
# Anthropic: claude-sonnet-4-5-20250929, claude-3-5-sonnet-20241022
# OpenRouter: meta-llama/llama-3.1-8b-instruct, anthropic/claude-3.5-sonnet
# Gemini: gemini-1.5-pro, gemini-1.5-flash
DEFAULT_MODEL=claude-sonnet-4-5-20250929

# ============================================
# Performance Settings
# ============================================

MAX_TOKENS=4096
TEMPERATURE=0.7
TIMEOUT=300000

# ============================================
# Feature Flags
# ============================================

ENABLE_STREAMING=true
ENABLE_HOOKS=true
ENABLE_NEURAL=true

# ============================================
# Logging
# ============================================

LOG_LEVEL=info
VERBOSE=false
