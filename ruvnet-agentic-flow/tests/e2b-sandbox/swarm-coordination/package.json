{
  "name": "swarm-coordination-e2b-tests",
  "version": "1.0.0",
  "description": "E2B sandbox testing for swarm coordination agents",
  "scripts": {
    "test": "jest --coverage",
    "test:hierarchical": "jest hierarchical-coordinator.test.ts",
    "test:mesh": "jest mesh-coordinator.test.ts",
    "test:adaptive": "jest adaptive-coordinator.test.ts",
    "test:integration": "jest integration.test.ts",
    "test:watch": "jest --watch",
