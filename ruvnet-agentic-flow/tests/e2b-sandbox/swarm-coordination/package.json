    "benchmark": "npm test -- --testNamePattern=\"Performance|Scalability\""
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "swarm",
    "coordination",
    "e2b",
    "testing",
    "flash-attention",
    "byzantine",
    "adaptive"
  ],
  "author": "Agentic Flow",
