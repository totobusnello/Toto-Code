{
  "description": "Default simulation configuration",
  "swarm": {
    "topology": "mesh",
    "maxAgents": 5,
    "communication": "memory"
  },
  "database": {
    "mode": "graph",
    "path": "simulation/data",
    "dimensions": 384,
    "autoMigrate": true
  },
  "llm": {
    "provider": "openrouter",
    "model": "anthropic/claude-3.5-sonnet",
    "temperature": 0.7,
    "maxTokens": 4096
  },
  "streaming": {
    "enabled": false,
    "source": "@ruvector/agentic-synth",
    "bufferSize": 1000
  },
  "optimization": {
    "enabled": true,
    "batchSize": 10,
    "parallel": true,
    "caching": true
  },
  "reporting": {
    "format": "json",
    "verbosity": 2,
    "includeMetrics": true,
    "includeLogs": false
  }
}
