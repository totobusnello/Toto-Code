# Benchmark Configuration
TEST_SCENARIO=concurrent          # concurrent, workspace-isolation, conflict-resolution, operation-log-sync
AGENT_COUNT=10                    # Number of concurrent agents
COMMIT_COUNT=100                  # Commits per agent
CONFLICT_RATE=10                  # Percentage of commits that conflict (0-100)
BENCHMARK_DURATION=600            # Maximum benchmark duration in seconds

# Jujutsu Configuration
JJ_WORKSPACE_MODE=shared-log      # shared-log, clone-per-agent, memory-mapped

# Performance Tuning
ENABLE_METRICS=true               # Collect detailed performance metrics
ENABLE_PROFILING=false            # Enable CPU/memory profiling
LOG_LEVEL=info                    # debug, info, warn, error

# Resource Limits (per container)
GIT_MEM_LIMIT=4g
GIT_CPU_LIMIT=2
JJ_MEM_LIMIT=4g
JJ_CPU_LIMIT=2
COORDINATOR_MEM_LIMIT=2g
COORDINATOR_CPU_LIMIT=1
AGENTDB_MEM_LIMIT=4g
AGENTDB_CPU_LIMIT=2
