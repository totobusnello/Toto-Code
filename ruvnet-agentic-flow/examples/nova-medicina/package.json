{
  "name": "nova-medicina",
  "version": "1.0.0",
  "description": "AI-powered medical analysis system with anti-hallucination safeguards - supplement to professional healthcare",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "nova-medicina": "./bin/nova-medicina"
  },
  "scripts": {
    "start": "node bin/nova-medicina",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm test",
    "generate-docs": "node docs/generate-docs.js"
  },
  "keywords": [
    "medical-ai",
    "healthcare",
    "medical-analysis",
    "anti-hallucination",
    "clinical-decision-support",
    "evidence-based-medicine",
    "medical-verification",
    "healthcare-ai",
    "patient-safety",
    "medical-nlp",
    "clinical-validation",
    "ai-safety",
    "medical-assistant",
    "healthcare-technology",
    "agentic-flow"
  ],
  "author": "ruv (github.com/ruvnet, ruv.io)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruvnet/nova-medicina.git"
  },
  "bugs": {
    "url": "https://github.com/ruvnet/nova-medicina/issues"
  },
  "homepage": "https://github.com/ruvnet/nova-medicina#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "agentic-flow": "^2.0.0",
    "agentdb": "^1.0.0",
    "claude-flow": "^2.7.0",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "boxen": "^7.1.1",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "agentic-flow": "^2.0.0"
  }
}
