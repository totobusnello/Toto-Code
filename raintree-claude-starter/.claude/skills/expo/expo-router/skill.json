  "dependencies": {
    "recommended": ["expo"],
    "complements": ["supabase", "stripe"]
  },
  "collaboration": {
    "canProvideDataTo": ["expo"],
    "canConsumeDataFrom": ["expo", "supabase"],
    "sharedContext": ["routes", "navigation_state", "params", "auth_flow"]
  }
