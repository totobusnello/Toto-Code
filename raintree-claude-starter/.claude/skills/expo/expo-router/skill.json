{
  "id": "expo-router",
  "version": "1.0.0",
  "category": "mobile",
  "keywords": ["expo", "router", "navigation", "file-based"],
  "dependencies": ["expo"],
  "docs": null,
  "semanticTags": {
    "primary": ["navigation", "file_based_routing", "universal_navigation"],
    "secondary": ["deep_linking", "layouts", "dynamic_routes"],
    "domains": ["mobile", "web", "navigation", "frontend"]
